<%= link_to "<< Back to List", posts_path %>

<div class="post new">
  <h2>New Post</h2>

  <%= simple_form_for @post, url: posts_path do |f| %>
  

    <%= render(:partial => 'form', :locals => {:f => f}) %>

    <div class="form-buttons">
      <%= submit_tag("Next (Qualifications)") %>
    </div>

  <% end %>
</div>